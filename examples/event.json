{
  "event":
    {
    "EventTypeIndicator": {
       "description": "Indicates type of event e.g.  Scheduled, Estimated, Actual",
       "required": true,
       "type": "text"
    },
    "DateTime": {
       "description": "Date and time of the event",
       "required": true,
       "type": "datatime"
    },
    "AirWaybillNumber": {
       "description": "Air Waybill number",
       "type": "text"
    },
    "AirWaybillReference": {
       "description": "Air Waybill URI",
       "type": "undefined"
    },
    "EventCode": {
       "description": "Movement or milestone code. Refer CXML Code List 1.18, e.g. DEP, ARR, FOH, RCS",
       "type": "text"
    },
    "EventName": {
       "description": "If no EventCode provided, event name - e.g. Security clearance",
       "type": "text"
    },
    "PerformedBy": {
       "description": "Party performing the event",
       "type": "company.json"
    },
    "Location": {
       "description": "Location of event",
       "type": "location.json"
    },
    "HandlingDetails": {
       "description": "Handling details",
       "type": "handling.json"
    }
  }
}